#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5d75a712f4f0 .scope module, "tb_decoder3to8" "tb_decoder3to8" 2 3;
 .timescale -9 -12;
v0x5d75a717a200_0 .var "a", 2 0;
v0x5d75a717a2c0_0 .net "y", 7 0, v0x5d75a717a0c0_0;  1 drivers
S_0x5d75a712f680 .scope module, "uut" "decoder3to8" 2 9, 3 1 0, S_0x5d75a712f4f0;
 .timescale -9 -12;
    .port_info 0 /INPUT 3 "a";
    .port_info 1 /OUTPUT 8 "y";
v0x5d75a7169fd0_0 .net "a", 2 0, v0x5d75a717a200_0;  1 drivers
v0x5d75a717a0c0_0 .var "y", 7 0;
E_0x5d75a716ac50 .event anyedge, v0x5d75a7169fd0_0;
    .scope S_0x5d75a712f680;
T_0 ;
    %wait E_0x5d75a716ac50;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5d75a717a0c0_0, 0, 8;
    %pushi/vec4 1, 0, 1;
    %ix/getv 4, v0x5d75a7169fd0_0;
    %store/vec4 v0x5d75a717a0c0_0, 4, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5d75a712f4f0;
T_1 ;
    %vpi_call 2 16 "$dumpfile", "decoder3to8.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5d75a712f4f0 {0 0 0};
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x5d75a717a200_0, 0, 3;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x5d75a717a200_0, 0, 3;
    %delay 10000, 0;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x5d75a717a200_0, 0, 3;
    %delay 10000, 0;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x5d75a717a200_0, 0, 3;
    %delay 10000, 0;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x5d75a717a200_0, 0, 3;
    %delay 10000, 0;
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x5d75a717a200_0, 0, 3;
    %delay 10000, 0;
    %pushi/vec4 6, 0, 3;
    %store/vec4 v0x5d75a717a200_0, 0, 3;
    %delay 10000, 0;
    %pushi/vec4 7, 0, 3;
    %store/vec4 v0x5d75a717a200_0, 0, 3;
    %delay 10000, 0;
    %vpi_call 2 29 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
